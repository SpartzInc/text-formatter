{
    "name": "cmgmyr/text-formatter",
    "description": "Easily formats a post title",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Gmyr",
            "email": "cmgmyr@gmail.com",
            "homepage": "http://www.chrisgmyr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Cmgmyr\\TextFormatter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cmgmyr\\TextFormatter\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
